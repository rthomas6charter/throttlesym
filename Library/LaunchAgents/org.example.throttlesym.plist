<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>KeepAlive</key>
    <true/>
    <key>Disabled</key>
    <false/>
    <key>Label</key>
    <string>org.example.throttlesym</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/sudo</string>
        <string>/Applications/throttlesym.app/Contents/Resources/throttlesym/bin/throttlesym.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/throttlesym.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/throttlesym.out</string>
</dict>
</plist>